<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/dauberjd93/templates/master/minecraftserver.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>GameServers:</Category>
  <Name>dauberjd-skyfactory4-minecraftserver</Name>
  <Description>
        skyfactory4-minecraftserver
  </Description>
  <Overview>
    skyfactory4-minecraftserver
  <Support></Support>
  <Registry>https://hub.docker.com/r/dauberjd93/mineserversf4</Registry>
  <GitHub>https://github.com/dauberjd93/atskyfactory4</GitHub>
  <Repository>dauberjd93/atskyfactory4</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>25565</HostPort>
        <ContainerPort>25565</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>MAX_BACKUPS</Name>
      <Value>10</Value>
    </Variable>
    <Variable>
      <Name>JAVA_INITIAL_HEAP_SIZE</Name>
      <Value>8192M</Value>
    </Variable>
    <Variable>
      <Name>JAVA_MAX_HEAP_SIZE</Name>
      <Value>8192M</Value>
    </Variable>
    <Variable>
      <Name>JAVA_MAX_THREADS</Name>
      <Value>3</Value>
    </Variable>
    <Variable>
      <Name>UMASK</Name>
      <Value>000</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/config</HostDir>
      <ContainerDir>/opt/minecraft/server</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
</Containers>